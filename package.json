        "vue": "^2.5.0",
        "vue-codemirror": "^4.0.5",
        "vue-computed-helpers": "^1.3.0",
        "vue-drag-and-drop": "^3.0.0",
        "vue-plyr": "^4.0.0",
        "vue2-dropzone": "^3.2.2"
    }
